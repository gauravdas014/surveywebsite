<style>
    body {
        overflow-x: hidden;
    }
    .welcome-text {
        text-align: center;
        margin-top: 50px;
        padding: 0 20px;
    }

    .welcome-text-2 {
        text-align: center;
        margin-top: 50px;
    }

    .main-section{
        width: 600px;
        margin: 50px auto;
        padding: 50px;
    }

    .submit-question-btn{
        background-color: #0275d8;
        color: white;
        font-size: 18px;
        border: none;
        padding: 8px 20px;
        margin-top: 10px;
        border-radius: 5px;
    }

    table {
    font-family: arial, sans-serif;
    border-collapse: collapse;
    width: 100%;
    }

    td, th {
    border: 1px solid #dddddd;
    text-align: left;
    padding: 8px;
    }

    tr:nth-child(even) {
    background-color: #dddddd;
    }

    .main {
        padding: 50px;
    }

    @media only screen and (max-width: 600px) {
        .main-section{
        width: 90%;
        margin: 50px auto;
        padding: 40px;
    }

    .welcome-text {
        text-align: center;
        margin-top: 30px;
        padding: 0 20px;
    }

    .welcome-text-2 {
        text-align: center;
        margin-top: 30px;
    }
}

</style>
