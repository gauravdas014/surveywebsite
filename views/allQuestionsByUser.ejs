                  <th>Verified</th>
                </tr>
                      <% for(let i=0; i < allQuestions.algoQs.length; i++) { %>
                         <tr>
                           <td><%= i+1 %></td>
                           <td><%= allQuestions.algoQs[i].question %></td>
                           <td><%= allQuestions.algoQs[i].answer %></td>
                           <td> 
                              <% if(allQuestions.algoQs[i].isVerified){ %>
                                  <span style="color: green;">&#9989;</span> &nbsp;
                                  &nbsp; &nbsp;
                                <% } else{ %>  
                                  <span style="color: red;">&#10060; </span>
